<h1><%= @cat.name %></h1>

<table>
  <tr>
    <th>Attributes</th>
    <th>Values</th>
  </tr>
  <% @columns.each do |attr_name| %>
    <tr>
      <td><%= attr_name %></td>
      <td><%= @cat.send(attr_name) %></td>
    </tr>
  <% end %>
</table>

<a href="<%= edit_cat_url(@cat) %>">Edit <%= @cat.name %></a>

<h2>RENTAL REQUESTS</h1>
<table>
  <tr>
    <th>Start Date</th>
    <th>End Date</th>
    <th>Status</th>
  </tr>
  <% @cat.rental_requests.order('start_date').each do |request| %>
    <%= render 'req_tr', req: request %> 
  <% end %>
</table>